// --- LOOKUP FUNCTION (top-level) ---
getValue(_dates, _values) =>
    var float res = na
    for i = 0 to array.size(_dates) - 1
        if time == array.get(_dates, i)
            res := array.get(_values, i)
    res

// --- RETRIEVE BAND VALUES (top-level) ---
bandP2 = getValue(dateArray, p2Array)
bandP1 = getValue(dateArray, p1Array)
bandFV = getValue(dateArray, fvArray)
bandM1 = getValue(dateArray, m1Array)
bandM2 = getValue(dateArray, m2Array)

// === Blue-Only Color Scheme & Fills (all at top-level) ===
baseCol = #009AFF

p2 = plot(bandP2, title="+2 SD", color=color.new(baseCol, 65), linewidth=2)
p1 = plot(bandP1, title="+1 SD", color=color.new(baseCol, 45), linewidth=2)
fv = plot(bandFV, title="Fair Value", color=color.new(baseCol, 5), linewidth=2)
m1 = plot(bandM1, title="-1 SD", color=color.new(baseCol, 45), linewidth=2)
m2 = plot(bandM2, title="-2 SD", color=color.new(baseCol, 65), linewidth=2)

// fills must be at top-level, not indented under any function or block:
fill(p2, p1, color=color.new(baseCol, 90), title="+2 to +1 SD")
fill(p1, fv, color=color.new(baseCol, 50), title="+1 SD to Fair Value")
fill(fv, m1, color=color.new(baseCol, 50), title="Fair Value to -1 SD")
fill(m1, m2, color=color.new(baseCol, 90), title="-1 to -2 SD")
